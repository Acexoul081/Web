type User{
    id: ID!
    username: String!
    email: String!
    pass: String!
    profilePic: String!
    membershipId: ID!
}


input newUser{
    username: String!
    email: String!
    pass: String!
    profilePic: String!
}

extend type Query{
    users:[User!]!
    getUser(id:ID!):User!
}

extend type Mutation{
    createUser(input:newUser): User!
    updateUser(id: ID!, input:newUser):User!
    deleteUser(id: ID!):Boolean!
}